    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": ">=7",
    "jest": "^27.5.1",
    "lint-staged": ">=10",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "typescript": "^4.7.4",
    "@types/node": "^18.0.0"
  },
  "dependencies": {
    "set-cookie-parser": "^2.5.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
